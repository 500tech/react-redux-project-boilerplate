{
  "name": "react-redux-boilerplate",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">8.11.0",
    "npm": ">5.5.1"
  },
  "dependencies": {
    "@500tech/react-scripts": "3.4.1",
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@formatjs/intl-pluralrules": "2.2.10",
    "@formatjs/intl-relativetimeformat": "5.2.10",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/jest": "26.0.3",
    "@types/lodash": "4.14.157",
    "@types/node": "12.11.1",
    "@types/react": "16.9.41",
    "@types/react-dom": "16.9.8",
    "@types/react-intl": "3.0.0",
    "@types/react-loadable": "5.5.3",
    "@types/react-redux": "7.1.9",
    "@types/react-router": "5.1.8",
    "@types/react-router-dom": "5.1.5",
    "@types/redux-actions": "2.6.1",
    "@types/redux-devtools": "3.0.47",
    "@types/superagent": "4.1.8",
    "@types/url-join": "4.0.0",
    "emotion-theming": "10.0.27",
    "history": "5.0.0",
    "http-server": "0.12.3",
    "lodash": "4.17.15",
    "moment": "2.27.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-intl": "4.7.5",
    "react-loadable": "5.5.0",
    "react-redux": "7.2.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-actions": "2.6.5",
    "redux-freeze": "0.1.7",
    "redux-logger": "3.0.6",
    "reselect": "4.0.0",
    "superagent": "5.3.1",
    "url-join": "4.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:staging": "env-path -p .env.staging react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "remove-typescript": "react-scripts remove-typescript",
    "prettier:all": "prettier --write 'src/**/*.{js,ts,tsx}'",
    "heroku-postbuild": "npm run build",
    "start:heroku": "http-server -p ${PORT:=8080} build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "none",
    "bracketSpacing": true,
    "jsxBracketSameLine": true,
    "requirePragma": false
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-links": "5.3.19",
    "@storybook/addon-viewport": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/preset-create-react-app": "3.1.2",
    "@storybook/preset-typescript": "3.0.0",
    "@storybook/react": "5.3.19",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.2",
    "husky": "4.2.5",
    "jest-enzyme": "7.1.2",
    "lint-staged": "10.2.11",
    "mimic": "2.0.9",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "react-docgen-typescript-loader": "3.7.2",
    "storybook-addon-intl": "2.4.1",
    "typescript": "3.9.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "post-commit": "git reset",
      "pre-commit": "lint-staged"
    }
  }
}
